<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Nelayan List</title>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
</head>
<body>
  <%- include('partials/navbar', {pageName : 'Nelayan List'})%>
  <div class ="container">
  <h1>Nelayan List</h1>
  <br><br><br>
  <a href="/nelayan/add"><button type="button" class="btn btn-info btn-sm">Add Nelayan</button></a>
  <br><br><br>
  <table class="table table-hove">
    <thead class="thead-dark">
      <tr>
        <th style="text-align:center">Nama Nelayan</th>
        <th style="text-align:center">Action</th>
      </tr>
    <thead>
      <tbody>
        <% data.forEach(element => { %>
          <tr>
            <td style="text-align:center"><%=element.namaNelayan%></td>
            <td style="text-align:center">
            <a href="/nelayan/edit/<%=element.id%>"><button type="button" class="btn btn-info btn-sm">Edit</button></a> 
            <a href="/nelayan/delete/<%=element.id%>"><button type="button" class="btn btn-info btn-sm">Hapus</button></a>
            </td>
          </tr>
          <% })  %>
        </tbody>   
        </table>
    </div>
    <%- include('partials/footer')%>
  </body>
  </html>